{
  "name": "avia-origin",
  "description": "Intuitive framework for the lilybird library",
  "version": "1.0.1",
  "type": "module",
  "author": {
    "name": " Upsided",
    "email": "matthewlloydw@gmail.com",
    "url": "https://upsided.net"
  },
  "main": "./src/index.ts",
  "scripts": {
    "start": "bun .",
    "dev": "bun --watch .",
    "build": "bun run build.mjs",
    "format": "prettier --write .",
    "prepublishOnly": "rm -rf dist && bun run build",
    "cliffjump": "bun format && bun cj"
  },
  "keywords": [
    "bun",
    "lilybird",
    "framework",
    "discord"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "dependencies": {
    "@lilybird/handlers": "^0.5.0",
    "@lilybird/jsx": "^0.3.0",
    "@lilybird/transformers": "^0.4.1",
    "@types/bun": "^1.1.3",
    "@types/minimist": "^1.2.5",
    "@types/shell-quote": "^1.7.5",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "lilybird": "^0.7.1",
    "minimist": "^1.2.8",
    "shell-quote": "^1.8.1"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^4.0.2",
    "bun-plugin-dts": "^0.2.3",
    "prettier": "3.3.1",
    "typescript": "^5.4.5"
  }
}
